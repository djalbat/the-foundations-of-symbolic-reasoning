padding: 0;
display: flex;
row-gap: 128pt;
align-items: center;
flex-direction: column;
justify-content: center;
background-colour: #1a5f1a;

primaryHeading,
secondaryHeading {
  width: 400pt;
  colour: #e9f3e9;
  text-align: center;
  font-weight: normal;
  font-family: "Computer Modern Serif";
  text-transform: uppercase;
}

primaryHeading {
  font-size: 40pt;
  line-height: 60pt;
}

secondaryHeading {
  font-size: 24pt;
}
